<?xml version="1.0" encoding="utf-8"?>
<component name="Route" extends="Group">
    <interface>
        <field id="path" type="string" />
        <field id="component" type="string" />

        <function name="show" />
        <function name="hide" />

        <function name="open" />
        <function name="close" />
    </interface>
    <children>
        <Group id="componentTarget" />
        <Group id="skeletonTarget" />
    </children>
</component>
