{
  "name": "@tkss/roact-router",
  "version": "0.0.23",
  "description": "A lodash inspired utility for Roku development",
  "main": "index.js",
  "keywords": [
    "ropm",
    "roku"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TKSS-Software/roact-router.git"
  },
  "scripts": {
    "preversion": "npm run build",
    "build": "npm run compile && ts-node ./scripts/createDist.ts",
    "compile": "npx bsc",
    "docs": "npm run build && jsdoc -c jsdoc.json -d docs -t ./node_modules/better-docs",
    "build-test-project": "cd test-project && npx bsc"
  },
  "author": "TKSS Software",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "ropm": {
    "packageRootDir": "dist",
    "rootDir": "src"
  },
  "devDependencies": {
    "@jsdoc/salty": "^0.2.8",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.16",
    "better-docs": "^2.7.3",
    "braintree-jsdoc-template": "^3.3.0",
    "brighterscript": "^0.67.5",
    "brighterscript-jsdocs-plugin": "^0.7.3",
    "concat": "^1.0.3",
    "copyfiles": "^2.4.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "merge-files": "^0.1.2",
    "replace-in-files": "^3.0.0",
    "ropm": "^0.10.25",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "bsc-plugin-auto-findnode": "^0.1.0",
    "promises": "npm:@rokucommunity/promises@^0.3.0",
    "rodash": "npm:@tkss/rodash@^0.0.34"
  }
}
